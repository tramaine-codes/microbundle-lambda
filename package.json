{
  "name": "microbundle-lambda",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib dist",
    "bundle": "npm run clean && microbundle build -i src/lambda.ts -o dist/lambda.js --target node --format esm --generateTypes false --sourcemap --external oracledb",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "Tramaine L. Gillus",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.31.0",
    "@types/node": "22.16.5",
    "@vitest/coverage-v8": "3.2.4",
    "del-cli": "6.0.0",
    "eslint": "9.31.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "microbundle": "0.15.1",
    "prettier": "3.6.2",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.37.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "dayjs": "1.11.13",
    "oracledb": "6.9.0"
  }
}
