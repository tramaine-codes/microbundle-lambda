{
  "name": "microbundle-lambda",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run clean && npm test && tsc",
    "clean": "del-cli lib dist",
    "bundle": "npm run clean && microbundle build -i src/lambda.ts -o dist/lambda.js --target node --format esm --generateTypes false --sourcemap --external oracledb",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "Tramaine L. Gillus",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "20.4.0",
    "@commitlint/config-conventional": "20.4.0",
    "@eslint/js": "9.39.2",
    "@types/node": "24.10.9",
    "@vitest/coverage-v8": "4.0.18",
    "del-cli": "7.0.0",
    "eslint": "9.39.2",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "microbundle": "0.15.1",
    "prettier": "3.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "dayjs": "1.11.19",
    "oracledb": "6.10.0"
  }
}
